    <style name="FloatsDialogText" parent="@style/MaterialAlertDialog.Material3.Body.Text">
        <item name="android:fontFamily">@font/inter</item>
    </style>

    <style name="FloatsDialogButton" parent="@style/Widget.Material3.Button.TextButton.Dialog">
        <item name="fontFamily">@font/inter</item>
    </style>
